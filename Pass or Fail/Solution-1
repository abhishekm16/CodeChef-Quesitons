#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        int n,x,p;
        cin>>n>>x>>p;
        int total = x*3 + (n-x)*(-1);
        if(total >= p){
            cout<<"pass"<<endl;
        }else{
            cout<<"fail"<<endl;
        }
    }
    return 0;
}
